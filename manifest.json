{
    "manifest_version": 3,
    "name": "Highli5",
    "version": "1.0",
    "permissions": ["tabs","activeTab", "scripting", "contextMenus"],
    "background": {
    "service_worker":
      "background.js"
    },
    "browser_action": {
      "default_popup": "popup.html"
    },
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["content.js"]
      }
    ]
}